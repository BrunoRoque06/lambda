{
  "env": {
    "commonjs": true
  },
  "plugins": ["jest", "ramda"],
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:ramda/recommended",
    "prettier"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "prefer-const": "off" // `const` is misleading because the object itself can be mutated, just that the reference cannot change. Good for native types though
  }
}
